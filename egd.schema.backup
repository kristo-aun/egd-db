--

CREATE FUNCTION f_hepburn_replace_long_vowels(roumaji text) RETURNS text
    LANGUAGE plpgsql
    AS $$
	DECLARE
	BEGIN
		roumaji := replace(roumaji, 'ā', 'aa');
		roumaji := replace(roumaji, 'ē', 'ee');
		roumaji := replace(roumaji, 'ī', 'ii');
		roumaji := replace(roumaji, 'ō', 'ou');
		roumaji := replace(roumaji, 'ū', 'uu');	
		RETURN roumaji;
	END;
$$;


