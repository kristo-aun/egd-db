    a.word AS txt,
    a.word_pos AS pos,
    a.word_translation AS gloss,
    a.comment,
    a.word_reading AS rdng,
    a.id,
    a.with_jmdict,
    count(b.id) AS kanji_count,
    array_to_string(array_agg(b.kanji ORDER BY b.id), ' / '::text) AS ktxt_kanjis
   FROM (ilo a
     LEFT JOIN v_mtm_ilo_kanji b ON ((a.id = b.ilo_id)))
  GROUP BY a.id;


